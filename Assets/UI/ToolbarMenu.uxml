                    <ui:Button text="Button" display-tooltip-when-elided="true" style="width: 70px; height: 70px; background-color: rgba(88, 88, 88, 0); font-size: 0; background-image: url(&apos;project://database/Assets/pics/5.png?fileID=2800000&amp;guid=6f2e11dfb1803409da3058d9135824cc&amp;type=3#5&apos;); margin-bottom: -19px; margin-top: -55px;" />
